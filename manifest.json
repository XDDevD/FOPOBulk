{
    
    "manifest_version": 2,
    "name": "FOPOBulk",
    "version": "0.120.05.2",
    "icons": {
        "16": "fopo.png",
        "48": "fopo.png",
        "128": "fopo.png"
    },
    "browser_action": {
        "default_popup": "main.html",
        "default_icon": "fopo.png"
    },
    
  "permissions": ["tabs", "*://*.fopo.com.ar/*"],
  "content_scripts": [
    {
      "matches": ["*://*.fopo.com.ar/*"],
      "js": ["fopoBulk.js"]
    }
  ]
}